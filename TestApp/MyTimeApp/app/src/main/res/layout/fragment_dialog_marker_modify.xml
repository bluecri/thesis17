<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sample.thesis17.mytimeapp.map.DialogMarkerModifyFragment">

    <EditText
        android:id="@+id/fragment_dialog_marker_modify_marker_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginTop="16dp"
        />

    <EditText
        android:id="@+id/fragment_dialog_marker_modify_marker_memo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginTop="4dp"
        android:fontFamily="sans-serif"
        />

    <Button
        android:id="@+id/fragment_dialog_marker_modify_marker_button_selectType"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="타입선택" />

</FrameLayout>
